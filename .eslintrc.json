{
     "root": true,
     "parserOptions": {
          "ecmaVersion": 12,
          "sourceType": "module"
     },
     "extends": [
          "eslint:recommended",
          "prettier",
          "expo"
     ],
     "env": {
          "es2021": true,
          "node": true
     },
     "plugins": [
          "prettier"
     ],
     "rules": {
          "no-console": "error",
          "prettier/prettier": "error"
     }
}